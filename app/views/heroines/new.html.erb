<h1>Create New Heroine</h1>

<%= form_with({ model: @heroine }) do |f| %>
<div>
    <label> Name </label>
    <%= f.text_field(:name) %>
</div>
<div>
    <label> Super Name</label>
    <%= f.text_field(:super_name) %>
</div>
<div>   
    <label> Power </label>
    <%= f.collection_select(:power_id, @powers, :id, :name) %>
</div>
    <%= f.submit %>
<% end %>
